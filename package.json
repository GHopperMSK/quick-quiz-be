{
    "name": "quick-quiz-be",
    "version": "0.0.0",
    "description": "Highly customizable quiz tool for websites (backend)",
    "private": true,
    "main": "dist/app.js",
    "scripts": {
        "clean": "rimraf dist/*",
        "copy-assets": "ts-node tools/copyAssets",
        "tsc": "./node_modules/typescript/bin/tsc",
        "build": "npm-run-all clean tsc copy-assets",
        "dev:start": "npm-run-all build start",
        "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
        "start": "node ."
    },
    "keywords": [
        "quiz",
        "questionnaire",
        "survey"
    ],
    "author": "Poriadin Nikolai",
    "license": "MIT",
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "deepmerge": "^4.2.2",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1",
        "pg": "^8.8.0",
        "pug": "2.0.0-beta11"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "@types/fs-extra": "^9.0.13",
        "@types/node": "^16.18.11",
        "@types/pg": "^8.6.6",
        "@types/shelljs": "^0.8.11",
        "fs-extra": "^10.1.0",
        "nodemon": "^2.0.20",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
